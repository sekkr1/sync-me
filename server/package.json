    "@types/shortid": "0.0.29",
    "@types/socket.io": "^2.1.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "discord.js": "^11.4.2",
    "dotenv": "^6.2.0",
